    <script>
        $(document).ready(function () {
            BindAccountDetails();
            BindNotifications();
        });

        function BindAccountDetails() {
            var _dataURL = "/Registration/AccountDetails/";
            $.ajax({
                type: 'POST',
                url: _dataURL,
                data: "{}",
                contentType: "application/json;charset=utf-8",
                dataType: "json",
                success: function (data) {
                    var Length = data.length;
                    if (Length > 0) {
                        var EmailID = data[0].EmailId;
                        var Location = data[0].Location;
                        var FirstName = data[0].FirstName;
                        var LastName = data[0].LastName;
                        if (Location == "" || Location == null) {
                            Location = "../Content/assets/images/Default.png";
                        }
                        $("#lblEmailID").text(EmailID);
                        $("#imgProfilePicture").attr("src", Location);
                        $("#lblLoginUserName").text(FirstName + " " + LastName);
                    }
                },
                error: function (data) {
                    alert('Failed');
                    return false;
                }
            });
        }

        function BindNotifications() {
            $("#spanNotificationCount").text("3");
            var list = '<li class="unread">' +
                          '<a href="@Url.Action("sendmail","mail", new { id = 0 })">' +
                             '<span class="label label-info"><i class="fa fa-comment"></i></span>Shankar' +
                                 'Sent a message<span class="pull-right text-muted small">23 mins ago</span>' +
                           '</a>' +
                        '</li>';
            $("#ulBindNotifications").append(list);
        }
    </script>

